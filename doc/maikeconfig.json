{
	"maikeconfig": {
		"env": {
			"AR": "ar",
			"AS": "as",
			"CC": "cc",
			"CXX": "g++",
			"LD": "ld"
		},
		"source_file_info_loaders": {
			"cxx": {
				"compiler": {
					"command": {
						"args": [
							"-c",
							"?-std=%cxx:cxxversion%",
							"*-%cxx:cflags%",
							"*-iquote%cxx:iquote%",
							"*-I%cxx:includedir%",
							"-DMAIKE_JOB_ID=%maike:jobid%",
							"-o",
							"%maike:target%",
							"%maike:source%"
						],
						"executable": "%env:CXX%"
					},
					"cxxversion_max": "",
					"cxxversion_min": ""
				}
			}
		},
		"source_tree_loader": {
			"file_info_loaders": {
				".cpp": "cxx",
				".hpp": "cxx"
			},
			"input_filter": [
				"^\\.",
				"^__"
			],
			"recursive": 1
		},
		"utils": {
			"pkg-config": {},
			"vcs": {
				"get_branch": {
					"args": [
						"rev-parse",
						"--abbrev-ref",
						"HEAD"
					],
					"executable": "git"
				},
				"get_revision": {
					"args": [
						"rev-parse",
						"HEAD"
					],
					"executable": "git"
				},
				"get_version_tag": {
					"args": [
						"describe",
						"--dirty"
					],
					"executable": "git"
				}
			}
		}
	}
}
