{
    "directoryoptions":{
        "paths_full_reject":[
			"doc",
            "test",
			"gh-pages",
			".git"
        ],
        "recursive":1
    },
    "source_files":[
        "."
    ],
    "target_hooks":[
        {
            "config":{
                "appcompile":{
                    "args":[
                        "-g",
                        "-fpic",
                        "{cxxversion}",
                        "-Wall",
                        "-Woverflow",
                        "-Wconversion",
                        "-Werror",
                        "{includedir}",
                        "{libdir}",
                        "-o",
                        "{target}",
                        "{source}",
                        "{dependencies}"
                    ],
                    "name":"g++"
                },
				"cflags_format":"-^",
                "cflags_extra":[],
                "cxxversion_max":-1,
                "cxxversion_min":201103,
                "dllcompile":{
                    "args":[
                        "-g",
                        "-fpic",
                        "{cxxversion}",
                        "-Wall",
                        "-Woverflow",
                        "-Wconversion",
                        "-Werror",
                        "{includedir}",
                        "{libdir}",
                        "-shared",
                        "-o",
                        "{target}",
                        "{source}",
                        "{dependencies}"
                    ],
                    "name":"g++"
                },
                "includedir":[],
                "includedir_format":"-I^",
                "includedir_noscan":[],
                "libcompile":{
                    "args":[
                        "rcs",
                        "{target}",
                        "{dependencies}"
                    ],
                    "name":"ar"
                },
                "libdir":[],
                "libdir_format":"-L^",
                "libext_format":"-l^",
                "libint_format":"-l:^",
                "objcompile":{
                    "args":[
                        "-c",
                        "-g",
                        "-fpic",
                        "{cxxversion}",
                        "-Wall",
                        "-Woverflow",
                        "-Wconversion",
                        "-Werror",
                        "{includedir}",
                        "-o",
                        "{target}",
                        "{source}"
                    ],
                    "name":"g++"
                },
                "pkgconfig":{
                    "args":[
                        "{action}",
                        "{libname}"
                    ],
                    "name":"pkg-config"
                },
                "stdprefix":"-std=",
                "versionquery":{
                    "args":[
                        "-E",
                        "-dM",
                        "-x",
                        "c++",
                        "{nullfile}"
                    ],
                    "name":"g++"
                }
            },
            "filename_exts":[
                ".c++",
                ".cc",
                ".cpp",
                ".cxx",
                ".h",
                ".h++",
                ".hh",
                ".hpp",
                ".hxx"
            ],
            "name":"cxxdefault",
            "plugin":"targetcxx"
        },
        {
            "config":{
                "command":{
                    "args":[
                        "--",
                        "{script}",
                        "{args}"
                    ],
                    "name":"python3"
                }
            },
            "filename_exts":[
                ".py"
            ],
            "name":"pythondefault",
            "plugin":"targetpython"
        }
    ],
    "targetinfo":{
        "target_directory":"__targets"
    }
}