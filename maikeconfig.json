{
	"maikeconfig": {
		"dir_compiler": {
			"config": {},
			"recipe": "recipes/make_directory.py"
		},
		"source_file_info_loaders": {
			"app": {
				"compiler": {
					"config": {
						"cflags": [
							"-g"
						]
					},
					"recipe": "recipes/cxx_linker.py"
				},
				"loader": "maikerule"
			},
			"cxx": {
				"compiler": {
					"config": {
						"cflags": [
							"-c",
							"-g",
							"-Wall",
							"-Wextra",
							"-Werror"
						],
						"iquote": [
							"."
						]
					},
					"recipe": "recipes/cxx_compiler.py"
				},
				"loader": "cxx_src_loader"
			},
			"extension": {
				"compiler": {
					"config": {},
					"recipe": ""
				},
				"loader": "extension"
			}
		},
		"source_tree_loader": {
			"file_info_loaders": {
				".app.maikerule": "app",
				".cpp": "cxx",
				".hpp": "cxx",
				".py": "extension"
			},
			"input_filter": [
				"^\\.",
				"^__",
				"build.py"
			],
			"recursive": 0
		}
	}
}
