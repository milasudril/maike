{
"targetconfig":
	{
	"target_directory":"__targets"
	}

,"spider":
	{
	"start_directory":"."
	}

,"directoryloader":
	{
	 "paths_reject":[".git",".codelite",".clang","test","Debug","Release","test"
		,"__targets"]
	,"recursive":1
	}

,"cxxoptions":
	{
	 "includedir":[]
	,"includedir_noscan":[]
	,"libdir":[]
	,"cxxversion_min":201103
	,"stdprefix":"-std="
	,"platform_suffix":"x86_64-linux-gnu"
	,"includedir_format":"-I^"
	,"libdir_format":"-L^"
	,"libext_format":"-l^"
	,"libint_format":"-l:^"
	,"objcompile":
		{
		 "name":"g++"
		,"args":["-c","-g","-fpic","{cxxversion}","-Wall","-Woverflow"
			,"-Wconversion","-Werror","{includedir}","-o","{target}","{source}"]
		}
	,"appcompile":
		{
		 "name":"g++"
		,"args":["-g","-fpic","{cxxversion}","-Wall","-Woverflow","-Wconversion"
			,"-Werror","{includedir}","{libdir}","-o","{target}","{dependencies}"]
		}
	,"versionquery":
		{
		 "name":"g++"
		,"args":["-E","-dM","-x","c++","{nullfile}"]
		}
	}
}
